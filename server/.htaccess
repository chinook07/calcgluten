<IfModule mod_rewrite.c>

    RewriteEngine On
    RewriteBase /
    RewriteRule ^$ calcgluten-43v9.onrender.com/:3000/ [P,L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-l
    RewriteRule ^(.*)$ calcgluten-43v9.onrender.com:3000/$1 [P,L]

</IfModule>