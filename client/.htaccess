<IfModule mod_rewrite.c>

    RewriteEngine On
    RewriteBase /
    RewriteCond %{HTTP_HOST} ^www\.touristechezsoi\.ca$ [NC]
    RewriteRule ^(.*)$ https://touristechezsoi.ca/$1 [R=301,L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-l
    RewriteRule . /index.html [L]
    RewriteCond %{HTTP_HOST} ^calcgluten\.touristechezsoi\.ca$ [NC]
    RewriteRule ^(.*)$ /calcgluten/$1 [R=301,L]

</IfModule>